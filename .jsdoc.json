{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false,
    "disableSort": false,
    "referenceTitle": "meteoJS",
    "default": {
      "outputSourceFiles": true
    }
  },
  "opts": {
    "destination": "./doc/",
    "private": true,
    "recurse": true
  }
}